package listaL;

import java.util.Scanner;

public class ON1 {

	public static void main(String[] args) {
    Scanner entrada = new Scanner(System.in);
    String cont="sim";
    
    while(cont.equals("sim")) {
    	
    	
    	
    	
    	System.out.println("Deseja realizar uma nova passagem?");
    	cont = entrada.next();
    }
    
	if(cont.equalsIgnoreCase("nao")) {
		System.out.println("RESUMO DO DIA");
	}
		
	}

}
