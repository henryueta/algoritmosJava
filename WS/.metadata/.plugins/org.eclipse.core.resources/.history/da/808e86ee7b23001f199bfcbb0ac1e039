package listaL;


import java.util.Random;
import java.util.Scanner;

public class L0 {

	public static void main(String[] args) {
    Scanner entrada = new Scanner(System.in);
	Random sorteador = new Random();
    int lista[] = new int[6];
    int num=0;
	int lista2[] = new int[6];
    int lista3[] = new int[6];
    int lista4[] = new int[6];

	
	for(int i = 0;i < lista3.length;i++){
	   lista[i]= sorteador.nextInt(1, 12);
	   num = i +1;
	   lista2[i]= sorteador.nextInt(0, 13);
	   lista3[i]= sorteador.nextInt(0, 15);

	   
	   lista4[i]=lista2[i]+lista[i]+lista3[i];
	   
	   if(lista4[2] == lista4[num]) {
		   lista4[i] = sorteador.nextInt(1,61);
	   }
	   
	}
	
	for(int i = 0; i< lista.length; i++) {
		System.out.println("\n"+lista4[i]);
		System.out.println("A"+lista2[i]);
		System.out.println("B"+lista[i]);
		System.out.println("C"+lista[i]);


	}
	}
	
	

}
