    int lista[] = new int[6];
    int num=0;
    int aposta1[] = new int[6];
	
    for (int i = 0; i < aposta1.length; i++) {
    	num = i +1;
		System.out.println("Aposte o "+num+ " nÃºmero: ");
		aposta1[i]= entrada.nextInt();
		System.out.println(lista[i]);
	}
    
    
    
	for(int i = 0;i < lista.length;i++){
	   lista[i]= sorteador.nextInt(1, 61);
	   System.out.println(lista[i]);
	}
	
	
	
	
	}
	
	

